<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/css/ui-lightness/jquery-ui-1.8.17.custom.css")" rel="stylesheet"
        type="text/css" />
</head>
<body>
    <div id="header">
        <div id="title">
            <img src="@Url.Content("~/images/paidthx-alpha-transparent.png")" height="60px" />
        </div>
        <div id="logindisplay">
            
            @Html.Partial("_LogOnPartial")
        </div>
    </div>
    <div id="main-wrapper">
        <div id="left-col">
            <div id="menucontainer">
                <ul id="submenu">
                    <li>@Html.ActionLink("Overview", "Index", "Home")</li>
                    <li>@Html.ActionLink("Transactions", "Index", "Payment")</li>
                    <li>@Html.ActionLink("Batch Files", "Index", "PaymentFile")</li>
                    <li>@Html.ActionLink("Members", "Index", "User")</li>
                    <li>@Html.ActionLink("Administration", "Index", "Administration")</li>
                    <li>@Html.ActionLink("Logs", "Index", "Log")</li>
                </ul>
            </div>
        </div>
        <div id="main-col">
            <div id="main">
                @RenderBody()
            </div>
        </div>
    </div>
    <div id="footer">
    </div>
</body>
</html>
