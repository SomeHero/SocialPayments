@model Mobile_PaidThx.Models.PaystreamModels.PaystreamModel
@{
    ViewBag.Title = "Your PaidThx Profile";
}
<script src="@Url.Content("~/Scripts/paystream.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/complete-profile-dialog.js")" type="text/javascript"></script>
<div id="container">
    <div id="header">
    <dl id="main-tabs" class="tabs contained" data-role="listview">
        <dd>
            <a href="#simpleContained1" class="first">Send $</a></dd>
        <dd>
            <a href="#simpleContained2">Request $</a></dd>
        <dd>
            <a href="#simpleContained3">Paystream</a></dd>
        <dd>
            <a href="#simpleContained4" class="active tab-settings"></a></dd>
    </dl>
    </div>
    <div id="content-container">
        <ul class="tabs-content contained">
            <li id="simpleContained1Tab">
                <div id="main-container">
                    @{Html.RenderPartial("../Paystream/PartialViews/SendMoney");}
                    @{Html.RenderPartial("_footer");}
                </div>
            </li>
            <li id="simpleContained2Tab">
                <div id="main-container">
                    @{Html.RenderPartial("../Paystream/PartialViews/RequestMoney");}
                    @{Html.RenderPartial("_footer");}
                </div>
            </li>
            <li id="simpleContained3Tab">
                <div id="main-container">
                     @{Html.RenderPartial("../Paystream/PartialViews/SubNav", Model);}
                    @{Html.RenderPartial("_footer");}
                </div>
            </li>
            <li class="active" id="simpleContained4Tab">
                <div id="main-container">
                     @{Html.RenderPartial("PartialViews/SubNav", Model.ProfileModel, new ViewDataDictionary());}
                     @{Html.RenderPartial("_footer");}
                </div>
            </li>
        </ul>
    </div>
</div>
        
