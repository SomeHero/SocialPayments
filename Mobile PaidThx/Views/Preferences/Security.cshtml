@{
    ViewBag.Title = "Security &amp; Privacy";
    ViewBag.PageID = "security-page";
    ViewBag.Classes = "settings";
}
<div data-role="page" id="@ViewBag.PageID" class="@ViewBag.Classes" @TempData["DataUrl"]>
    @{ Html.RenderPartial("TitleBar"); }
    <div data-role="header" data-position="fixed" class="header-pdthx" data-theme="c">
        <h1>
            Settings</h1>
        <!--<a href="./Preferences" id="btn-settings" class="top-button ui-btn-right"></a>-->
    </div>
    <div data-role="content">
        <div class="pad">
            <ul data-role="listview" data-inset="true">
                <li>
 
                    <a href="@Url.Content("~/Preferences/UserAgreement")" data-prefetch data-ajax="false" data-transition="slide">
                    Change Password </a></li>
                <li>
                    <!-- Sign Out -->
                    <a  href="@Url.Content("~/Preferences/SignOut")" data-prefetch data-ajax="false" data-transition="pop">
                    Change Security Pin </a></li>
                <li>
                    <!-- Sign Out -->
                    <a  href="@Url.Content("~/Preferences/SignOut")" data-prefetch data-ajax="false" data-transition="pop">
                    Forgot Security Pin</a></li>
            </ul>
        </div>
    </div>
</div>
